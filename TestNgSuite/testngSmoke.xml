<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="Smoke"></include>
		</run>
	</groups>


	<listeners>
		<listener class-name="utils.Listeners"></listener>
	</listeners>

	<test thread-count="5" name="validatePreferenceAlert">
		<classes>
			<class name="DnT.Appium_Poc.validatePreferenceAlert" />
		</classes>
	</test> <!--
	Test -->

	<test thread-count="5" name="verifyUserScrollingAction">
		<classes>
			<class name="DnT.Appium_Poc.verifyUserScrollingAction" />
		</classes>

	</test> <!--
	Test -->
	<test thread-count="5" name="ViewslongPressTest">
		<classes>
			<class name="DnT.Appium_Poc.ViewslongPressTest" />
		</classes>

	</test> <!--
	Test -->
</suite> <!--
Suite -->
